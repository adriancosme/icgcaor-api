{
  "name": "icgcaor-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:build": "docker-compose -f database.yml up",
    "db:rebuild": "docker-compose -f database.yml build",
    "db:start": "docker-compose -f database.yml start",
    "db:stop": "docker-compose -f database.yml stop",
    "db:debug": "docker-compose -f database.yml logs -f",
    "deploy:now": "npm run build && now",
    "deploy:docker:build": "docker-compose -f deploy.yml up",
    "deploy:docker:down": "docker-compose -f deploy.yml down",
    "deploy:docker:rebuild": "docker-compose -f deploy.yml build",
    "deploy:docker:rebuild:nocache": "docker-compose -f deploy.yml build --no-cache",
    "deploy:docker:recreate": "docker-compose -f deploy.yml up --force-recreate",
    "deploy:docker:start": "docker-compose -f deploy.yml start",
    "deploy:docker:stop": "docker-compose -f deploy.yml stop",
    "deploy:docker:debug": "docker-compose -f deploy.yml debug",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "commit": "git-cz",
    "crun": "node dist/main-cli"
  },
  "dependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@hapi/joi": "17.1.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "csv-writer": "^1.6.0",
    "express-rate-limit": "^6.5.2",
    "helmet": "^6.0.0",
    "joi": "^17.6.0",
    "mongoose": "^6.13.6",
    "nest-commander": "^3.20.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.31.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^11.1.9",
    "@types/bull": "^3.15.9",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "~4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
